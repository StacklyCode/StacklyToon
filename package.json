{
  "name": "boilerplate-next",
  "version": "1.0.0",
  "description": "",
  "type": "commonjs",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx|d.ts)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx|d.ts)\" --write",
    "ul": "npm uninstall @stacklycore/ui && npm i @stacklycore/ui",
    "pre-commit": "lint-staged"
