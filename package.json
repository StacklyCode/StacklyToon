    "brain.js": "^2.0.0-beta.15",
    "formik": "^2.2.9",
    "framer-motion": "^6.3.11",
    "jotai": "^1.7.2",
    "js-cookie": "^3.0.1",
    "next": "^12.1.6",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
